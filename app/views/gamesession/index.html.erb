<table style="width:100%">
  <tr>
    <th>ID</th>
    <th>Host</th>
    <th>Date</th>
    <th>Game</th>
    <th>City</th>
    <th>PayerNB</th>
    <th>MaxPlayers</th>
    <th>Time</th>
    <th>PlayerSkill</th>
    <th>Status</th>
  </tr>
  <%@sessions.each do |session|%>
    <tr>
      <td><%= session.id %></td>
      <td><%= session.host.nickname %></td>
      <td><%= session.date %></td>
      <td><%= session.game.title %></td>
      <td><%= session.city %></td>
      <td><%= session.playernb %></td>
      <td><%= session.maxplayers %></td>
      <td><%= session.time %></td>
      <td><%= session.playerskill %></td>
      <td><%= session.status %></td>
      <td><%= link_to 'Show', gamesession_path(session) %></td>
      <% if session.players.include?(current_user)%>
        <td><%=link_to "LeaveGame", leavegame_path(id: session.id)%></td>
      <% else %>
        <td><%=link_to "JoinGame", joingame_path(id: session.id)%></td>
      <% end %>
    </tr>
  <%end%>
</table>
<%=link_to "CreateGame", new_gamesession_path %>
